	amlogic-dt-id = "xiaomi_sc2_jaws";
	compatible = "amlogic, sc2";
	interrupt-parent = <0x01>;
	#address-cells = <0x02>;
	#size-cells = <0x02>;

	custom_maps {
		mapnum = <0x02>;
		map0 = <0x02>;
		map1 = <0x03>;
		phandle = <0x40>;

		map_0 {
			mapname = "xiaomi-ir-0";
			customcode = <0x86>;
			release_delay = <0x50>;
			size = <0x0c>;
			keymap = <0x50067 0x6006c 0xb0069 0xc006a 0xd001c 0x80066 0x7000f 0x4008b 0x1100ec 0x300f0 0xe0073 0xf0072>;
