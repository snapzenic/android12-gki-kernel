				i2c1_pins4 {

					mux {
						groups = "i2c1_sda_h6\0i2c1_scl_h7";
						function = "i2c1";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c2_pins1 {

					mux {
						groups = "i2c2_sda_x\0i2c2_scl_x";
						function = "i2c2";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
					};
				};

				i2c2_pins2 {

					mux {
						groups = "i2c2_sda_z10\0i2c2_scl_z11";
						function = "i2c2";
						drive-strength-microamp = <0xbb8>;
						bias-disable;
